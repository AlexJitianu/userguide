<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task
  PUBLIC "-//OASIS//DTD DITA Task//EN" "http://docs.oasis-open.org/dita/v1.1/OS/dtd/task.dtd">
<task id="named-user-license-registration-text-file">
    <title>Named User License Registration with Text File</title>
    <taskbody>
        <steps>
            <step>
                <cmd>Save the license key in a file named <filepath>licensekey.txt.</filepath></cmd>
            </step>
            <step>
                <cmd>Copy <filepath>licensekey.txt.</filepath> in the installation folder of  <ph
            keyref="product"/> or in the <filepath>lib</filepath> subfolder of the installation
          folder.</cmd>
            </step>
            <step>
                <cmd> Start the <ph keyref="product"/>. </cmd>
            </step>
        </steps>
    </taskbody>
</task>
